{
  "name": "laterwatch",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev:app": "tauri dev",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "tauri": "tauri",
